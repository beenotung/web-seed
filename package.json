{
  "name": "web-seed",
  "version": "0.0.0",
  "main": "dist/index.js",
  "watch": {
    "lint": "src/index.ts"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:ts": "tsc -p .",
    "build:js": "mkdir -p dist && browserify build/index.js > index.js",
    "lint": "tslint -p .",
    "fix": "tslint -p . --fix",
    "pretest": "run-s build lint",
    "test": "node dist/index.js",
    "preserve": "npm run build",
    "serve": "http-server -o -s -c-1",
    "watch": "run-p watch:*",
    "watch:npm": "npm-watch",
    "watch:ts": "tsc -p . --watch",
    "dev": "run-p watch serve"
  },
  "author": "Beeno Tung",
  "license": "BSD-3-Clause",
  "description": "Illustrate the routing algorithm of Link State Routing",
  "dependencies": {
    "@beenotung/tslib": "^8.5.0"
  },
  "devDependencies": {
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@types/node": "^8.0.51",
    "browserify": "^14.5.0",
    "codelyzer": "^4.0.1",
    "http-server": "^0.10.0",
    "npm-run-all": "^4.1.2",
    "npm-watch": "^0.3.0",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.6.1",
    "typestub-horizon-client": "^0.1.8"
  }
}
